{
    "index_name": "dev_lotusdocs.dev",
    "sitemap_urls": [
        "https://lotusdocs-staging.vercel.app/sitemap.xml"
    ],
    "start_urls": [
        "https://lotusdocs-staging.vercel.app/docs/"
    ],
    "stop_urls": [
        ".*(?<!/)$"
    ],
    "scrape_start_urls": false,
    "use_anchors": true,
    "strip_chars": "#",
    "selectors_exclude": [
        "i",
        ".material-icons",
        "#sidebar",
        "#toc",
        "#list-item",
        "#doc-nav",
        "#back-to-top"
    ],
    "selectors": {
        "lvl0": {
            "selector": "//li[@class=\"breadcrumb-item breadcrumb-link text-capitalize active\"]/../a",
            "type": "xpath",
            "global": true,
            "default_value": "Documentation"
        },
        "lvl1": ".docs-content .content-title .title-text",
        "lvl2": "#content h2",
        "lvl3": "#content h3",
        "lvl4": "#content h4",
        "lvl5": "#content h5",
        "text": ".docs-content .lead,#content p,#content ul,#content ol,#content .alert,#content blockquote"
    }
}